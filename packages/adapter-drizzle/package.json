{
	"name": "adapter-drizzle",
	"version": "0.0.3",
	"description": "Drizzle Adapter",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch"
	},
	"keywords": [
		"drizzle"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"types": "./index.d.ts",
	"files": [
		"*.d.ts*",
		"*.js",
		"src"
	],
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js"
		}

	},
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {

		"@libsql/client": "0.5.6",
		"@types/better-sqlite3": "7.6.9",
		"better-sqlite3": "9.4.3",
		"drizzle-kit": "^0.20.14",
		"drizzle-orm": "^0.30.4"
	},
	"peerDependencies": {
		"drizzle-orm": "0.30.x"
	}
}
